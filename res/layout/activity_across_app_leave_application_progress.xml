                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_aalogohorizon}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_aalogohorizon"/>
                <ImageView android:id="@+id/imageUntitleddesign"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_147pxh"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_63pxv"
                    android:layout_width="@dimen/_147pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_untitleddesign}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_untitleddesign"/>
