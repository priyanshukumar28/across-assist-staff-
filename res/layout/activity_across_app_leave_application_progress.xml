                    android:id="@+id/txtThankYou"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_84pxh"
                    android:layout_marginStart="@dimen/_84pxh"
                    android:layout_marginTop="@dimen/_116pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{acrossAppLeaveApplicationProgressVM.acrossAppLeaveApplicationProgressModel.txtThankYou}"
                    style="@style/txtRobotoromanregular16" tools:text="@string/lbl_thank_you"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_width="match_parent"
                    android:text="@{acrossAppLeaveApplicationProgressVM.acrossAppLeaveApplicationProgressModel.txtDescription}"
                    style="@style/txtRobotoromanregular14" tools:text="@string/msg_your_attendance"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
