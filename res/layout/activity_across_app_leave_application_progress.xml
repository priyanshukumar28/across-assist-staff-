<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="acrossAppLeaveApplicationProgressVM" type="com.acrossassistattendance.app.modules.acrossappleaveapplicationprogress.data.viewmodel.AcrossAppLeaveApplicationProgressVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumn1662038334044"
